<p id="notice"><%= notice %></p>

<h1>ノート</h1>
<%= form_for @search_form , :url => pages_path, :remote => true, :html => {:method => :get} do |f| %>
  <%= f.search_field :q %>
  <%= f.submit '検索' %>
<% end %>
<div class="pagination">
  <%= link_to 'コピーまたは、新規で新しいページを作る。', new_page_path %>
</div>
<div id="pages"><%= render @pages %></div>
<br>
<%= will_paginate @pages %>

