<% end %>
<input type="button" value="新規作成" class="btn btn-primary btn-block" onclick="location.href='<%= new_page_path  %>'" />
<hr>
<div class="col-xs-6 col-sm-6 col-md-4 col-lg-4" >       
  <input type="button" value="直近のページ編集" class="btn btn-default btn-block" onclick="location.href='<%= edit_page_path(@latest_page.id)%>'" />
</div>
<div class="col-xs-6 col-sm-6 col-md-4 col-lg-4" >       
  <input type="button" value="ページ一覧" class="btn btn-default btn-block" onclick="location.href='<%= pages_path%>'" />
</div>
<div class="col-xs-6 col-sm-6 col-md-4 col-lg-4" >       
